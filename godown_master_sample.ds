application "Godown Master Sample"
{
        date format = "dd-MMM-yyyy"
        time zone = "Asia/Kolkata"
        time format = "24-hr"
        forms
        {
                form godown_master
                {
                        displayname = "Godown Master"
                        success message = "Godown Master Added Successfully"
                        Section
                        (
                                type = section
                                row = 1
                                column = 0
                                width = medium
                        )
                        must have unique godown_code
                        (
                                type = text
                                displayname = "Godown Code"
                                row = 1
                                column = 1
                                width = medium
                        )
                        must have warehouse
                        (
                                type = text
                                displayname = "Warehouse"
                                row = 1
                                column = 1
                                width = medium
                        )
                        must have godown_name
                        (
                                type = text
                                displayname = "Godown Name"
                                row = 1
                                column = 1
                                width = medium
                        )
                        storage_condition
                        (
                                type = picklist
                                displayname = "Storage Condition"
                                maxchar = 100
                                values = {"Ambient","Cold","Hazardous"}
                                row = 1
                                column = 1
                                width = medium
                        )
                        capacity_uom
                        (
                                type = picklist
                                displayname = "Capacity UOM"
                                maxchar = 100
                                values = {"MT","KG","Litre","Unit"}
                                row = 1
                                column = 1
                                width = medium
                        )
                        capacity_value
                        (
                                type = decimal
                                displayname = "Capacity Value"
                                row = 1
                                column = 1
                                width = medium
                        )
                        must have batch_tracking_enabled
                        (
                                type = checkbox
                                displayname = "Batch Tracking Enabled"
                                row = 1
                                column = 1
                                width = medium
                        )
                        default_qc_hold_area
                        (
                                type = checkbox
                                displayname = "Default QC Hold Area"
                                row = 1
                                column = 1
                                width = medium
                        )
                        machinery
                        (
                                type = subform
                                displayname = "Machinery"
                                allow add entry = true
                                allow edit entry = true
                                allow delete entry = true
                                form = "godown_master_machinery"
                                row = 1
                                column = 1
                                width = large
                        )

                        actions
                        {
                                on add
                                {
                                        submit
                                        (
                                                type = submit
                                                displayname = "Submit"
                                        )
                                        reset
                                        (
                                                type = reset
                                                displayname = "Reset"
                                        )
                                }
                                on edit
                                {
                                        update
                                        (
                                                type = submit
                                                displayname = "Update"
                                        )
                                        cancel
                                        (
                                                type = cancel
                                                displayname = "Cancel"
                                        )
                                }
                        }
                }

                form godown_master_machinery
                {
                        displayname = "Machinery"
                        is subform = true
                        parentform = godown_master
                        Section
                        (
                                type = section
                                row = 1
                                column = 0
                                width = medium
                        )
                        must have machine_id
                        (
                                type = text
                                displayname = "Machine ID"
                                row = 1
                                column = 1
                                width = medium
                        )
                        must have machine_name
                        (
                                type = text
                                displayname = "Machine Name"
                                row = 1
                                column = 1
                                width = medium
                        )
                        must have category
                        (
                                type = picklist
                                displayname = "Category"
                                maxchar = 100
                                values = {"Capital Goods","Machine Spares","Production Line"}
                                row = 1
                                column = 1
                                width = medium
                        )
                        commission_date
                        (
                                type = date
                                displayname = "Commission Date"
                                row = 1
                                column = 1
                                width = medium
                        )
                        maintenance_vendor
                        (
                                type = textarea
                                displayname = "Maintenance Vendor"
                                row = 1
                                column = 1
                                width = medium
                        )
                        next_service_due
                        (
                                type = date
                                displayname = "Next Service Due"
                                row = 1
                                column = 1
                                width = medium
                        )
                        must have status
                        (
                                type = picklist
                                displayname = "Status"
                                maxchar = 100
                                values = {"Active","Under Maintenance","Retired"}
                                row = 1
                                column = 1
                                width = medium
                        )
                }
        }

        reports
        {
                default list godown_master_Report
                {
                        displayName = "All Godowns"
                        show all rows from godown_master
                        (
                                godown_code as "Godown Code"
                                warehouse as "Warehouse"
                                godown_name as "Godown Name"
                                storage_condition as "Storage Condition"
                                batch_tracking_enabled as "Batch Tracking"
                                default_qc_hold_area as "Default QC Hold"
                        )
                }
        }

        share_settings
        {
                "Read"
                {
                        name = "Read"
                        type = Users_Permissions
                        permissions = {Chat:true, Predefined:true, ApiAccess:true, PIIAccess:true, ePHIAccess:true}
                        description = "This profile will have read permission for all components\n"
                }
                "Write"
                {
                        name = "Write"
                        type = Users_Permissions
                        permissions = {Chat:true, Predefined:true, ApiAccess:true, PIIAccess:true, ePHIAccess:true}
                        description = "This profile will have write permission for all components\n"
                }
                "Admin"
                {
                        name = "Admin"
                        type = Users_Permissions
                        permissions = {Chat:true, Predefined:true, ApiAccess:true, PIIAccess:true, ePHIAccess:true}
                        description = "This profile will have admin permission for all components\n"
                }
                "Customer_Portal"
                {
                        name = "Customer Portal"
                        type = Customer_Portal
                        permissions = {Chat:true, Predefined:true, ApiAccess:true, PIIAccess:true, ePHIAccess:true}
                        description = "This is the default profile having only add and view permission.\n"
                }
                roles
                {
                        "CEO"
                        {
                                description = "User belonging to this role can access data of all other users."
                        }
                }
        }

        web
        {
                forms
                {
                        form godown_master
                        {
                                label placement = left
                        }
                }
                reports
                {
                        report godown_master_Report
                        {
                                layout
                                {
                                        type = 1
                                        datablock1
                                        {
                                                layout type = -2
                                                title = "Godown Overview"
                                                fields
                                                (
                                                        godown_code as "Godown Code"
                                                        warehouse as "Warehouse"
                                                        godown_name as "Godown Name"
                                                        storage_condition as "Storage Condition"
                                                        batch_tracking_enabled as "Batch Tracking"
                                                        default_qc_hold_area as "Default QC Hold"
                                                )
                                        }
                                        action
                                        (
                                                on click
                                                (
                                                        View Record
                                                )
                                                on right click
                                                (
                                                        Edit
                                                        Duplicate
                                                        Delete
                                                        Print
                                                        View Record
                                                )
                                        )
                                }
                        }
                }
                menu
                {
                        space Godown_Space
                        {
                                displayname = "Warehouse & Vendor"
                                icon = "business-store"

                                section Masters
                                {
                                        displayname = "Masters"
                                        icon = "design-app"
                                        form godown_master
                                        {
                                                icon = "files-archive"
                                        }
                                        report godown_master_Report
                                        {
                                                icon = "business-chart-bar-33"
                                        }
                                }
                                section App_Preferences
                                {
                                        displayname = "App Preferences"
                                        icon = "design-app"
                                        systemcomponent
                                        {
                                                type = localization
                                                displayname = "Language Selection"
                                                icon = "education-language"
                                        }
                                        systemcomponent
                                        {
                                                type = connection
                                                displayname = "Connections"
                                                icon = "technology-laptop"
                                        }
                                }
                        }
                }
                customize
                {
                        new theme = 11
                        font = "lato"
                        color options
                        {
                                color = "1"
                        }
                        logo
                        {
                                preference = "none"
                                placement = "left"
                        }
                }
        }

        phone
        {
                forms
                {
                        form godown_master
                        {
                                label placement = auto
                        }
                }
                customize
                {
                        layout = slidingpane
                        font = "default"
                        style = "3"
                        color options
                        {
                                color = lightred
                        }
                        logo
                        {
                                preference = "none"
                        }
                }
        }

        tablet
        {
                forms
                {
                        form godown_master
                        {
                                label placement = auto
                        }
                }
                customize
                {
                        layout = slidingpane
                        font = "default"
                        style = "3"
                        color options
                        {
                                color = lightred
                        }
                        logo
                        {
                                preference = "none"
                        }
                }
        }

        translation
        {
                {"Language_Settings":{"LANGAGUE_WITH_LOGIN":"browser"}}
        }
}
